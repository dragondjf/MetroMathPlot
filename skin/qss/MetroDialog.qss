QDialog{
	background: rgb(125, 125, 125);
}

QPushButton
{
	color: rgb(255, 255, 255);
	background-color: rgb(167, 205, 255);
	border:none;
    /*padding: 3px;*/
	font-family: "Verdana";
	font-size: 15px;
	text-align: center;
    width: 200px; 
    height: 50px;
}


QPushButton#OkButton:hover, QPushButton#OkButton:pressed , QPushButton#OkButton:checked
{
	background-color: rgb(85, 170, 255);

}
QPushButton#OkButton:hover
{
	background-image: url(skin/icons/dark/appbar.check.png);
	background-repeat:no-repeat;
	background-position: center left;
}
QPushButton#OkButton:pressed, QPushButton#OkButton:checked
{
	background-image: url(skin/icons/light/appbar.check.png);
	background-repeat:no-repeat;
	background-position: center right;
}

QPushButton#CancelButton:hover, QPushButton#CancelButton:pressed , QPushButton#CancelButton:checked
{
	background-color: rgb(85, 170, 255);
	text-align: center;
}
QPushButton#CancelButton:hover
{
	background-image: url(skin/icons/dark/appbar.cancel.png);
	background-repeat:no-repeat;
	background-position: center left;
}
QPushButton#CancelButton:pressed, QPushButton#CancelButton:checked
{
	background-image: url(skin/icons/light/appbar.cancel.png);
	background-repeat:no-repeat;
	background-position: center right;
}

